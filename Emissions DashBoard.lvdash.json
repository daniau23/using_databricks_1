{
  "datasets": [
    {
      "name": "4aa9468b",
      "displayName": "Location Data",
      "queryLines": [
        "-- use this method if you specify the schema name from the above tabs\r\n",
        "SELECT latitude, longitude, `GHG emissions mtons CO2e` as emissons \r\n",
        "from emissions_data_2023 \r\n",
        "-- OR\r\n",
        "-- SELECT latitude, longitude, `GHG emissions mtons CO2e` as emissons \r\n",
        "-- from emisssions.default.emissions_data_2023"
      ],
      "catalog": "emisssions",
      "schema": "default"
    },
    {
      "name": "4d080fe9",
      "displayName": "Emissions per Person",
      "queryLines": [
        "SELECT \r\n",
        "  county_state_name, \r\n",
        "  population, \r\n",
        "  -- Remove commas and cast emissions to DOUBLE\r\n",
        "  CAST(REPLACE(`GHG emissions mtons CO2e`, ',', '') AS DOUBLE) \r\n",
        "    -- Remove commas, cast population to DOUBLE, use NULLIF to avoid division by zero\r\n",
        "    / NULLIF(CAST(REPLACE(population, ',', '') AS DOUBLE), 0) AS emissions_per_person\r\n",
        "FROM emissions_data_2023\r\n",
        " ORDER BY emissions_per_person DESC"
      ],
      "catalog": "emisssions",
      "schema": "default"
    },
    {
      "name": "566c374a",
      "displayName": "Total Emissions per State",
      "queryLines": [
        "SELECT \r\n",
        "  state_abbr,\r\n",
        "  SUM(\r\n",
        "    CAST(\r\n",
        "      REPLACE(\r\n",
        "        `GHG emissions mtons CO2e`,\r\n",
        "        ',',\r\n",
        "        ''\r\n",
        "      ) AS DOUBLE\r\n",
        "    )\r\n",
        "  ) AS Total_Emissions\r\n",
        "FROM emissions_data_2023\r\n",
        "GROUP BY state_abbr\r\n",
        "ORDER BY Total_Emissions DESC\r\n",
        "-- LIMIT 10"
      ],
      "catalog": "emisssions",
      "schema": "default"
    },
    {
      "name": "92b46734",
      "displayName": "County Shaming",
      "queryLines": [
        "SELECT \r\n",
        "  county_state_name, \r\n",
        "  population, \r\n",
        "  -- Remove commas and cast emissions to DOUBLE\r\n",
        "  CAST(REPLACE(`GHG emissions mtons CO2e`, ',', '') AS DOUBLE) AS total_Emissions\r\n",
        "FROM emissions_data_2023\r\n",
        "ORDER BY total_emissions DESC\r\n",
        " LIMIT 10"
      ],
      "catalog": "emisssions",
      "schema": "default"
    }
  ],
